function loadplugs()
if isfile("Revs admin/auto/Plugins.txt") then
z = readfile("Revs admin/auto/Plugins.txt")
args = z:split(", ")
for i,v in(args) do
newtxtlabel(args[i])
end
end
end
    
loadplugs()

end


print("cmds are:")
for _,v in pairs(cmdslist) do
print(v)
end




function getPlayer(name)
local targets = {}
local plrstbl = game.Players:GetPlayers()
if name == "me" then
return {game.Players.LocalPlayer}
elseif name == "others" then
for i, v in pairs(plrstbl) do
if v ~= game.Players.LocalPlayer then
targets[#targets + 1] = v
end
end
elseif name == "all" then
for i, v in pairs(plrstbl) do
targets[#targets + 1] = v
end
elseif name == "random" then
for i, v in pairs(plrstbl) do
if v ~= game.Players.LocalPlayer then
return {plrstbl[math.random(1,#plrstbl)]}
end
end
else
name = name:lower()
for i, v in pairs(game.Players:GetPlayers()) do
if v.Name:lower():find(name) or v.DisplayName:lower():find(name) then
return {v}
end
end
end
return targets
end

function veryepictrol(x)
local prefix = getgenv().selectedprefix
local d,d1;d = x.Chatted:Connect(function(msg)
msg = msg:lower()
if string.sub(msg, 1, 3) == "/e " then
msg = string.sub(msg, 4)
end
if string.sub(msg, 1, 1) == prefix then
local cmd
local space = string.find(msg, " ")
if space then
cmd = string.sub(msg, 2, space - 1)
else
cmd = string.sub(msg, 2)
end

for _,v in pairs(plugins) do
if cmd == v then
if space ~= nil then
Who = string.sub(msg,space+1)
target = getPlayer(Who)[1]
end

if isfile("Revs admin/Plugins/"..v.."") then
loadstring(readfile("Revs admin/Plugins/"..v..""))()
elseif isfile("Revs admin/Plugins/"..v..".txt") then
loadstring(readfile("Revs admin/Plugins/"..v..".txt"))()
else
loadstring(readfile("Revs admin/Plugins/"..v..".lua"))()
end
end
end

if cmd == "discord" then
setclipboard("https://discord.gg/vWPzwRj2AE")
print("copied discord")
end

if cmd == "carpetloopgoto" then
for i,_ in pairs(loopgotoplayer) do
table.remove(loopgotoplayer,i)
end
Who = string.sub(msg, space + 1)
loopgototarget = getPlayer(Who)[1]
table.insert(loopgotoplayer,loopgototarget.Name)
getgenv().lg = rs.Heartbeat:Connect(function()
for _,v in pairs(loopgotoplayer) do
if game.Players:FindFirstChild(v) then
game.Players.LocalPlayer.Character:WaitForChild("HumanoidRootPart").CFrame = game.Players[v].Character:WaitForChild("HumanoidRootPart").CFrame * CFrame.Angles(1.5,0,0)
end
end
end)
end

if cmd == "uncarpetloopgoto" then
game.Players.LocalPlayer.Character:WaitForChild("HumanoidRootPart").Velocity = Vector3.new(0,0,0)
getgenv().lg:Disconnect()
game.Players.LocalPlayer.Character:WaitForChild("HumanoidRootPart").Velocity = Vector3.new(0,0,0)
for i,_ in pairs(loopgotoplayer) do
table.remove(loopgotoplayer,i)
end
end

if cmd == "lay" then
game.Players.LocalPlayer.Character:WaitForChild("Humanoid").Sit = true
wait(0.2)
for _,v in pairs(game.Players.LocalPlayer.Character.Humanoid.Animator:GetPlayingAnimationTracks()) do
v:Stop()
end
game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame * CFrame.Angles(1.5,0,0)
end

if cmd == "sit" then
game.Players.LocalPlayer.Character:WaitForChild("Humanoid").Sit = true
end

if cmd == "loopgoto" then
Who = string.sub(msg, space + 1)
loopgototarget = getPlayer(Who)[1]
getgenv().lg = rs.Heartbeat:Connect(function()
game.Players.LocalPlayer.Character:WaitForChild("HumanoidRootPart").CFrame = loopgototarget.Character:WaitForChild("HumanoidRootPart").CFrame
end)
end

if cmd == "unloopgoto" then
game.Players.LocalPlayer.Character:WaitForChild("HumanoidRootPart").Velocity = Vector3.new(0,0,0)
getgenv().lg:Disconnect()
game.Players.LocalPlayer.Character:WaitForChild("HumanoidRootPart").Velocity = Vector3.new(0,0,0)
for i,_ in pairs(loopgotoplayer) do
table.remove(loopgotoplayer,i)
end
end


if cmd == "speed" then
speed = string.sub(msg, space + 1)
game.Players.LocalPlayer.Character:WaitForChild("Humanoid").WalkSpeed = speed
end

if cmd == "cmds" then
cmdgui()
end

if cmd == "goto" then
Who = string.sub(msg, space + 1)
target = getPlayer(Who)[1]
game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = target.Character.HumanoidRootPart.CFrame
end

if cmd == "plugins" then
