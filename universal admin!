local plugins = {}

function loadplugin(file)
if isfile("Revs admin/Plugins/"..file.."") or isfile("Revs admin/Plugins/"..file..".txt") or isfile("Revs admin/Plugins/"..file..".lua") then
newtxtlabel(file)
if not isfile("Revs admin/auto/Plugins.txt") then
writefile("Revs admin/auto/Plugins.txt",file)
else
z = readfile("Revs admin/auto/Plugins.txt")
writefile("Revs admin/auto/Plugins.txt",""..z..", "..file.."")
end
else
print("file not found")
end
end
                
if not isfile("Revs admin") then
makefolder("Revs admin")
end

if not isfile("Revs admin/Plugins") then
makefolder("Revs admin/Plugins")
end

if not isfile("Revs admin/info.txt") then
writefile("Revs admin/info.txt","Revs admin made by jude_or_rev")
end

                
if not isfile("Revs admin/auto") then
makefolder("Revs admin/auto")
end

if isfile("Revs admin/auto/Plugins.txt") then
z = readfile("Revs admin/auto/Plugins.txt")
args = z:split(", ")
for i,v in(args) do
table.insert(plugins,args[i])
end
end

print("plugins:")
for _,v in(plugins) do
print(v)
end

print("there are "..#cmdslist.." cmds..")
print("there are "..#plugins.." plugins loaded..")

