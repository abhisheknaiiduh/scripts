function getPlayer(name)
local targets = {}
local plrstbl = game.Players:GetPlayers()
if name == "me" then
return {game.Players.LocalPlayer}
elseif name == "others" then
for i, v in pairs(plrstbl) do
if v ~= game.Players.LocalPlayer then
targets[#targets + 1] = v
end
end
elseif name == "all" then
for i, v in pairs(plrstbl) do
targets[#targets + 1] = v
end
elseif name == "random" then
for i, v in pairs(plrstbl) do
if v ~= game.Players.LocalPlayer then
return {plrstbl[math.random(1,#plrstbl)]}
end
end
else
name = name:lower()
for i, v in pairs(game.Players:GetPlayers()) do
if v.Name:lower():find(name) or v.DisplayName:lower():find(name) then
return {v}
end
end
end
return targets
end

function veryepictrol(x)
local prefix = getgenv().selectedprefix
local d,d1;d = x.Chatted:Connect(function(msg)
msg = msg:lower()
if string.sub(msg, 1, 3) == "/e " then
msg = string.sub(msg, 4)
end
if string.sub(msg, 1, 1) == prefix then
local cmd
local space = string.find(msg, " ")
if space then
cmd = string.sub(msg, 2, space - 1)
else
cmd = string.sub(msg, 2)
end

for _,v in pairs(plugins) do
if cmd == v then
if space ~= nil then
Who = string.sub(msg,space+1)
target = getPlayer(Who)[1]
end

if isfile("Revs admin/Plugins/"..v.."") then
loadstring(readfile("Revs admin/Plugins/"..v..""))()
elseif isfile("Revs admin/Plugins/"..v..".txt") then
loadstring(readfile("Revs admin/Plugins/"..v..".txt"))()
else
loadstring(readfile("Revs admin/Plugins/"..v..".lua"))()
end
end
end


if cmd == "suck" then
who = string.sub(msg, space + 1)
target = getPlayer(who)[1]
getgenv().suck = rs.Heartbeat:Connect(function()
game.Players.LocalPlayer.Character:WaitForChild("Humanoid").Sit = true
game.Players.LocalPlayer.Character:WaitForChild("HumanoidRootPart").CFrame = (target.Character:WaitForChild("HumanoidRootPart").CFrame * CFrame.Angles(0,math.rad(180),0) + target.Character:WaitForChild("HumanoidRootPart").CFrame.LookVector * 0.8) - Vector3.new(0,3,0)
game.Players.LocalPlayer.Character:WaitForChild("HumanoidRootPart").Velocity = Vector3.new(0,0,0)
end)
end

if cmd == "unsuck" then
if suck then
suck:Disconnect()
end
end


if cmd == "gfuck" then
who = string.sub(msg, space + 1)
target = getPlayer(who)[1]
getgenv().fuck = rs.Heartbeat:Connect(function()
game.Players.LocalPlayer.Character:WaitForChild("Humanoid").Sit = true
game.Players.LocalPlayer.Character:WaitForChild("HumanoidRootPart").CFrame = target.Character:WaitForChild("HumanoidRootPart").CFrame * CFrame.Angles(0,math.rad(180),0) + target.Character:WaitForChild("HumanoidRootPart").CFrame.LookVector * 0.8
game.Players.LocalPlayer.Character:WaitForChild("HumanoidRootPart").Velocity = Vector3.new(0,0,0)
end)
end

if cmd == "ungfuck" then
if fuck then
fuck:Disconnect()
end
end

if cmd == "dupetools" then
times = string.sub(msg, space + 1)
