for _, tool in pairs(game.Players.LocalPlayer.Backpack:GetChildren()) do
    if tool.CanBeDropped == true then
        tool.Parent = game.Players.LocalPlayer.Character
        table.insert(tools, tool)
    end
end

game.Players.LocalPlayer.Character:BreakJoints()

task.wait(0.5)

for _, tool in pairs(tools) do
    tool.Parent = workspace    
end

game.Players.LocalPlayer.CharacterAdded:wait()

for _, tool in pairs(tools) do
    game.Players.LocalPlayer.Character:WaitForChild("Humanoid"):EquipTool(tool)
    game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = oldpos
    game.Players.LocalPlayer.Character:WaitForChild(tool.Name).Parent = game.Players.LocalPlayer.Backpack
end
end

for i,_ in pairs(tools) do
table.remove(tools,i)
end
end

