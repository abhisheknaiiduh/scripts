game.Players.LocalPlayer.Character:WaitForChild("HumanoidRootPart").CFrame = (target.Character:WaitForChild("HumanoidRootPart").CFrame * CFrame.Angles(0,math.rad(180),0) + target.Character:WaitForChild("HumanoidRootPart").CFrame.LookVector * 0.8) - Vector3.new(0,3,0)
game.Players.LocalPlayer.Character:WaitForChild("HumanoidRootPart").Velocity = Vector3.new(0,0,0)
end)
end

